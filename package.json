{
  "name": "GitHave",
  "version": "1.0.0",
  "description": "一个AI驱动的代码仓库助手",
  "main": "./out/main/index.js",
  "author": "zhanghao",
  "homepage": "https://github.com/kinglegendzzh",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "dev": "electron-vite dev",
    "start": "electron-vite preview",
    "build:renderer": "vite build",
    "build": "electron-vite build",
    "build:mac": "npm run build && electron-builder --mac",
    "build:mac:arm": "npm run build && electron-builder --mac --arm64",
    "build:mac:x64": "npm run build && electron-builder --mac --x64",
    "build:mac:u": "npm run build && electron-builder --mac --universal",
    "build:win": "npm run build && electron-builder --win",
    "build:linux": "npm run build && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "ci:install": "npm ci",
    "ci:prune": "npm prune --omit=dev && electron-builder install-app-deps",
    "ci:dist:mac": "npm run ci:install && npm run build && npm run ci:prune && npx --yes electron-builder --mac --arm64 --x64",
    "ci:dist:win": "npm run ci:install && npm run build && npm run ci:prune && npx --yes electron-builder --win",
    "ci:dist:linux": "npm run ci:install && npm run build && npm run ci:prune && npx --yes electron-builder --linux"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@electron/remote": "^2.1.2",
    "@mdi/font": "^7.4.47",
    "@xterm/addon-attach": "^0.11.0",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-image": "^0.8.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "adm-zip": "^0.5.16",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.8.4",
    "chart.js": "^4.4.8",
    "chartjs-plugin-dragdata": "^2.3.1",
    "d3": "^7.9.0",
    "electron-devtools-installer": "^4.0.0",
    "electron-log": "^5.3.3",
    "electron-updater": "^6.3.9",
    "electron-vite": "^3.1.0",
    "element-plus": "^2.10.1",
    "file-type": "^20.5.0",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "http-server": "^14.1.1",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mammoth": "^1.9.0",
    "markdown-it": "^14.1.0",
    "markdown-it-mermaid": "^0.2.5",
    "mime-types": "^3.0.1",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "systeminformation": "^5.27.1",
    "tar": "^7.4.3",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-draggable-next": "^2.2.1",
    "vue-router": "^4.5.0",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vue3-treeselect": "^0.1.10",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.1.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "optionalDependencies": {
    "node-pty": "file:node-pty-1.0.0.tgz"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^5.2.3",
    "electron": "35.1.4",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^3.1.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^9.33.0",
    "mermaid": "^11.6.0",
    "monaco-editor": "^0.53.0",
    "monaco-editor-vue3": "^0.1.10",
    "prettier": "^3.5.3",
    "vite": "^6.2.3",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.5.13"
  }
}
